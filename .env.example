# .env.example
# AgentForge — Healthcare RCM AI Agent — Environment Variables
# ------------------------------------------------------------
# Copy this file to .env and fill in your own values.
# NEVER commit the real .env file to version control.

# Anthropic API key (required) — obtain from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenEMR base URL for the REST API integration (optional for local dev)
OPENEMR_BASE_URL=http://localhost:8300

# LangSmith observability (optional — set LANGCHAIN_TRACING_V2=false to disable)
LANGSMITH_API_KEY=your_langsmith_api_key
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=openemr-agent
